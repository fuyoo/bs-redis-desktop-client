{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:9000"
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": ["nsis", "appimage", "dmg", "app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "Release.entitlements",
      "exceptionDomain": "",
      "frameworks": []
    },
    "windows": {
      "nsis": {
        "compression": "zlib",
        "languages": ["SimpChinese"],
        "installerIcon": "icons/icon.ico",
        "minimumWebview2Version": "115.0.0"
      }
    }
  },
  "productName": "BS",
  "identifier": "link.xsa.bs",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/fuyoo/bs-redis-desktop-client/releases/latest/download/latest.json"
      ],
      "dangerousInsecureTransportProtocol": true,
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "BS",
        "width": 860,
        "height": 600,
        "minWidth": 860,
        "minHeight": 600,
        "center": true,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "visible": false
      }
    ]
  }
}
