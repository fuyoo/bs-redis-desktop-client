{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:9000"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "BS",
  "version": "2.0.0-1",
  "identifier": "link.xsa.bs",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "BS",
        "width": 860,
        "height": 600,
        "minWidth": 860,
        "minHeight": 600,
        "center": true,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay",
        "visible": false
      }
    ]
  }
}
